{
  "source": {
    "include": ["lib"],
    "includePattern": ".js$",
    "excludePattern": "(node_modules/|docs)"
  },
  "plugins": ["plugins/markdown", "node_modules/jsdoc-export-default-interop/dist/index"],
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": false,
    "default": {
            "outputSourceFiles": true
        }
  },
  "opts": {
    "recurse": true,
    "destination": "./docs/",
    "template": "./custom-template",
    "readme": "README.md"
  }
}